{
  "name": "subpirate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "stripe:webhook": "ts-node scripts/setup-stripe-webhook.ts",
    "stripe:sync": "node src/lib/stripe/sync-products.js",
    "stripe:verify": "node scripts/verify-stripe-production.js",
    "stripe:prod-setup": "npm run stripe:sync && npm run stripe:verify",
    "server": "node server.js",
    "webhook": "node webhook-server.js",
    "dev:webhook": "cross-env NODE_ENV=development STRIPE_TEST_MODE=true concurrently \"npm run dev\" \"npm run webhook\" \"stripe listen --forward-to http://localhost:4242/api/stripe/webhook\"",
    "stripe:test-env": "node scripts/test-stripe-environment.js",
    "stripe:migrate": "node scripts/migrate-stripe-to-production.js",
    "stripe:register-webhooks": "chmod +x scripts/register-product-price-webhooks.sh && ./scripts/register-product-price-webhooks.sh",
    "stripe:migrate-products": "ts-node scripts/migrate-products-to-stripe.ts",
    "admin:set": "node scripts/set-admin.js",
    "admin:remove": "node scripts/set-admin.js --remove",
    "gift:set": "node scripts/set-admin.js --gift",
    "gift:remove": "node scripts/set-admin.js --gift --remove",
    "admin:create-user-function": "node scripts/run_create_special_user_migration.js",
    "test:account-limits": "node scripts/test-account-limits.cjs",
    "db:test-limits": "echo 'To run the SQL tests, use: psql -h <host> -U <username> -d <database> -f migrations/test_account_limits.sql'"
  },
  "dependencies": {
    "@clerk/backend": "^1.24.2",
    "@clerk/clerk-react": "^5.23.0", 
    "@clerk/nextjs": "^6.12.0",
    "@clerk/types": "^4.46.1",
    "@radix-ui/react-slot": "^1.1.2",
    "@stripe/stripe-js": "^5.6.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/supabase-js": "^2.49.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/react-query": "^5.24.1",
    "@types/express": "^5.0.0",
    "@types/react-datepicker": "^6.0.1",
    "axios": "^1.6.7",
    "body-parser": "^1.20.3",
    "chart.js": "^4.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "firebase": "^11.3.1",
    "lucide-react": "^0.344.0",
    "next": "^15.1.7",
    "node-fetch": "^3.3.2",
    "openai": "^4.28.4",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-datepicker": "^6.1.0",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^5.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.22.2",
    "stripe": "^17.7.0",
    "svix": "^1.60.0",
    "tailwind-merge": "^3.0.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "chalk": "^5.4.1", 
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "supabase": "^1.145.4",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}